@model FoodCategoryCreateDto

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" method="post" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="CategoryName" class="control-label">Kateqoriya Adı</label>
                <input asp-for="CategoryName" class="form-control" />
                <span asp-validation-for="CategoryName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IConName" class="control-label">Şəkil</label>
                <input asp-for="IConName" class="form-control" type="file" />
                <span asp-validation-for="IConName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="@DeafultConstantValue.ADD_MESSAGE" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@DeafultConstantValue.RETURN_TO_BACK</a>
</div>